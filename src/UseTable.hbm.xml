<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="src.main.java.FacilitySystem">
	<class name="src.main.java.FacilitySystem.Use" table="use">
			<id name="id">
			<column name="id"></column>
			<generator class="identity">
			</generator>
		</id>
		<property name="StartDate" type="calendar" column="startdate" />
		<property name="useDescription" type="string" column="description" />
		<property name="EndDate" type="calendar" column="enddate" />
		<many-to-one name="facility" class="src.main.java.FacilitySystem.Facility" fetch="select" cascade="save-update, delete" column="facilityid"></many-to-one>
<!-- 		<property name=facility type="facility" column="facilityid"/>
 -->	</class>
</hibernate-mapping>